{
	"name": "lookberry/laravel-smsglobal-notifications-channel",
	"description": "SmsGlobal notification channel for Laravel 7, 8, 9",
	"type": "library",
	"license": "MIT",
	"keywords": [
		"laravel",
		"smsglobal"
	],
	"support": {
		"issues": "https://github.com/lookberry/laravel-smsglobal-notifications-channel/issues",
		"source": "https://github.com/lookberry/laravel-smsglobal-notifications-channel"
	},
	"authors": [
		{
			"name": "Salam Waddah",
			"email": "salamwaddah@gmail.com",
			"homepage": "https://salamwaddah.com/",
			"role": "Developer"
		},
		{
			"name": "Evgeny Tronchuk",
			"email": "evgenytronchuk@gmail.com",
			"homepage": "https://tronchuk.com/",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"SalamWaddah\\SmsGlobal\\": "src/",
			"Tests\\": "tests/"
		}
	},
	"require": {
		"php": ">=7.4",
		"illuminate/http": "^7.0 || ^8.0 || ^9.0 || ^10.0",
		"illuminate/notifications": "^7.0 || ^8.0 || ^9.0 || ^10.0",
		"illuminate/support": "^7.0 || ^8.0 || ^9.0 || ^10.0",
		"guzzlehttp/guzzle": "^6.5 || ^7.2",
		"hellochef-me/php-styles": "^1.0"
	},
	"minimum-stability": "stable",
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"orchestra/testbench": "^6.0"
	},
	"scripts": {
		"post-update-cmd": [
			"cp vendor/hellochef-me/php-styles/pre-commit .git/hooks/pre-commit",
			"chmod a+x .git/hooks/pre-commit"
		],
		"post-install-cmd": [
			"cp vendor/hellochef-me/php-styles/pre-commit .git/hooks/pre-commit",
			"chmod a+x .git/hooks/pre-commit"
		]
	},
	"version": "1.1.1"
}
